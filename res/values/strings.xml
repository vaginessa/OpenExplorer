    <string name="help_editor"></string>
    <string name="help_root">Open Explorer allows users who are using rooted Android devices to navigate directories that are "protected": /system (read-only by default), /data (read-write). Please note this can be dangerous and is not recommended unless you are an advanced user.</string>
    <string name="help_root_title">Root Operations</string>
    <string name="help_systemroot">Mounting your system as writeable can be dangerous and is not recommended for anybody but advanced users. You can easily damage/brick your device by making incorrect modifications. Please continue with highest caution.</string>
    <string name="help_systemroot_title">Rooted System Mount</string>
    
